[ExternalInterface="bindings_vectors"]
typedef extern Vector;

namespace simple {
  string hello(Pet pet);
  u64 add(u64 a, u64 b);
  string test_enum_to_string(TestEnum test_enum);
};

[Error]
enum SimpleError {
  "WrongDimensions",
};

dictionary Pet {
  string name;
};

enum TestEnum {
  "A",
  "B",
  "C",
};

interface Person {
  [Throws=SimpleError]
  constructor(string name, Vector location);
  void set_name(string name);
  string get_name();
  Vector get_location();
};
